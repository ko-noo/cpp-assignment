#pragma once
#include <iostream>

using namespace std;

class player { 
private:
	int att;
public:
	int hp;
	
	int setatt(int a) {
		att = a;
		return att;
	}

	void showatt() {
		cout << att;
	}
};

class monster {
private:
	int att;
public:
	int hp;

	int setatt(int a) {
		att = a;
		return att;
	}

	void showatt() {
		cout << att;
	}
};
